{
  "name": "Cloudflare Tunnel with DNS",
  "slug": "cloudflared_dns",
  "description": "Cloudflare Tunnel for Home Assistant with automatic DNS setup",
  "version": "1.0.0",
  "image": "ghcr.io/lpr-labs/cloudflared-dns-{arch}",
  "arch": ["amd64", "aarch64", "armv7"],
  "startup": "services",
  "boot": "auto",
  "ingress": false,
  "map": ["config:rw", "ssl:rw"],
  "options": {
    "api_token": "",
    "domain": "",
    "subdomain": "home",
    "tunnel_name": "homeassistant"
  },
  "schema": {
    "api_token": "password",
    "domain": "str",
    "subdomain": "str",
    "tunnel_name": "str"
  }
}
